<div id="term_demo"></div>

<script src="js/jqueryTerminal.js"></script>

<script>

jQuery(function($, undefined) {
    $('#term_demo').terminal(function(command) {
        if (command !== '') {
            try {
                console.log("holi");
                that = this
                // Ejecuta el comando
                // TODO: meter esto a un backend para poder probar que funke
                $.post("/authors/terminal",{
                    comando : command
                }, function (response){
                    that.echo(new String(response.message));
                });
                

            } catch(e) {
                this.error(new String(e));
            }
        } else {
            this.echo('');
        }
    }, {
        greetings: 'Bienvenido, este es un simulador de un servidor en bash desde la web!\n escribe los comandos bÃ¡sicos para empezar',
        name: 'js_demo',
        height: 200,
        prompt: 'usuario> '
    });
});

</script>