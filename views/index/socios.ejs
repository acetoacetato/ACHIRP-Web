<section id="sponsors" class="section-padding">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-12">
          <div class="section-title-header text-center">
            <h2 class="section-title wow fadeInUp" data-wow-delay="0.2s">Socios</h2>
            <p class="wow fadeInDown" data-wow-delay="0.2s"> 
              
            <% socios.forEach(dir => { %>
              <%= dir.nombre %>, <%= dir.institucion %><br>
            <% }) %>
             
          </div>
        </div>
      </div>
      
    </div>
  </section>